package program.practice;

public class CompareToDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name="Faraih";
		String name1="Faraih";
		String name2="Sunny";
		char ch='I';
		String strDouble;
		System.out.println("name Compare name1 is :"+name.compareTo(name1));
		System.out.println("name Compare name1 is :"+name.compareTo(name2));
		System.out.println(name.concat(name2));
		strDouble=String.format("%.2f", 44.56867);
		System.out.println(strDouble);


	}

}
